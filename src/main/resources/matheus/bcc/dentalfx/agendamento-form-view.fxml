<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<VBox prefHeight="724.0" prefWidth="768.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="matheus.bcc.dentalfx.AgendamentoFormController">
    <children>
        <VBox VBox.vgrow="ALWAYS">
            <children>
                <Label text="Dentista">
                    <VBox.margin>
                        <Insets top="8.0" />
                    </VBox.margin>
                </Label>
                <TextField fx:id="tf_dentista" editable="false" />
                <HBox prefHeight="72.0" prefWidth="720.0">
                    <children>
                        <VBox prefWidth="355.0">
                            <children>
                                <Label text="Data" />
                                <TextField fx:id="tf_data" editable="false" prefWidth="348.0" />
                            </children>
                            <HBox.margin>
                                <Insets right="50.0" />
                            </HBox.margin>
                        </VBox>
                        <VBox layoutX="10.0" layoutY="10.0" prefWidth="315.0">
                            <children>
                                <Label text="Horario" />
                                <TextField fx:id="tf_horario" editable="false" prefWidth="290.0" />
                            </children>
                        </VBox>
                    </children>
                    <opaqueInsets>
                        <Insets />
                    </opaqueInsets>
                    <VBox.margin>
                        <Insets top="20.0" />
                    </VBox.margin>
                </HBox>
                <VBox alignment="CENTER" prefHeight="458.0" prefWidth="720.0">
                    <children>
                        <Label text="Paciente">
                            <VBox.margin>
                                <Insets />
                            </VBox.margin>
                            <font>
                                <Font name="System Bold" size="22.0" />
                            </font>
                        </Label>
                        <TextField fx:id="tf_pesquisa" onKeyTyped="#onPesquisar" promptText="Informe o nome para buscar">
                            <VBox.margin>
                                <Insets top="10.0" />
                            </VBox.margin>
                        </TextField>
                        <TableView fx:id="table_view" prefHeight="373.0" prefWidth="720.0">
                            <columns>
                                <TableColumn fx:id="col_nome" prefWidth="486.0" text="NOME" />
                                <TableColumn fx:id="col_fone" minWidth="0.0" prefWidth="233.0" text="TELEFONE" />
                            </columns>
                            <VBox.margin>
                                <Insets top="20.0" />
                            </VBox.margin>
                        </TableView>
                    </children>
                </VBox>
            </children>
            <VBox.margin>
                <Insets bottom="12.0" left="12.0" right="12.0" top="12.0" />
            </VBox.margin>
        </VBox>
        <HBox alignment="CENTER" maxHeight="107.0" prefHeight="86.0" prefWidth="744.0" spacing="48.0">
            <children>
                <Button mnemonicParsing="false" onAction="#onConfirmar" prefHeight="33.0" prefWidth="132.0" text="Confirmar" />
                <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#onCancelar" prefHeight="33.0" prefWidth="132.0" text="Cancelar" />
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="12.0" left="12.0" right="12.0" top="12.0" />
    </padding>
</VBox>
