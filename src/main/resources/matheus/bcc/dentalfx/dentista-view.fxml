<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.ContextMenu?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER_LEFT" prefHeight="768.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="matheus.bcc.dentalfx.DentistaController">
    <children>
      <Label fx:id="label_boas_vindas" text="Boas vindas">
         <font>
            <Font name="System Bold" size="26.0" />
         </font>
      </Label>
      <MenuBar nodeOrientation="RIGHT_TO_LEFT">
         <menus>
            <Menu mnemonicParsing="false" text="Ajuda">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#onSobre" text="Sobre" />
                  <MenuItem mnemonicParsing="false" onAction="#onTopicos" text="Topicos" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Tema">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#onTrocarTema" text="Trocar Tema" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
        <HBox prefHeight="609.0" prefWidth="1160.0" spacing="36.0" VBox.vgrow="ALWAYS">
            <children>
                <VBox alignment="TOP_CENTER" spacing="10.0">
                    <children>
                        <TableView fx:id="tv_horarios" onMouseClicked="#onHorario" prefHeight="536.0" prefWidth="326.0" VBox.vgrow="ALWAYS">
                            <columns>
                                <TableColumn fx:id="col_hora" maxWidth="90.0" minWidth="90.0" prefWidth="90.0" text="HORA" />
                                <TableColumn fx:id="col_paciente" prefWidth="232.0" text="PACIENTE" />
                            </columns>
                            <columnResizePolicy>
                                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                            </columnResizePolicy>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                     <contextMenu>
                        <ContextMenu>
                          <items>
                              <MenuItem mnemonicParsing="false" onAction="#onImprimirFicha" text="Imprimir Ficha" />
                          </items>
                        </ContextMenu>
                     </contextMenu>
                        </TableView>
                        <Label text="Data da Consulta">
                     <VBox.margin>
                        <Insets top="24.0" />
                     </VBox.margin></Label>
                        <DatePicker fx:id="dp_dia_consulta" onAction="#onTrocouData" showWeekNumbers="true">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin></DatePicker>
                    </children>
                    <HBox.margin>
                        <Insets />
                    </HBox.margin>
                </VBox>
                <VBox HBox.hgrow="ALWAYS">
                    <children>
                  <TextArea fx:id="text_area" minHeight="100.0">
                     <VBox.margin>
                        <Insets bottom="24.0" />
                     </VBox.margin>
                  </TextArea>
                  <VBox prefWidth="798.0">
                     <children>
                        <TableView fx:id="tv_materiais">
                           <columns>
                              <TableColumn fx:id="col_material" maxWidth="1.7976931348623157E308" prefWidth="404.0" text="MATERIAL" />
                              <TableColumn fx:id="col_qtde_material" maxWidth="180.0" minWidth="180.0" prefWidth="180.0" text="QUANTIDADE" />
                           </columns>
                           <columnResizePolicy>
                              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                           </columnResizePolicy>
                        </TableView>
                        <HBox alignment="BOTTOM_LEFT" minHeight="30.0" prefHeight="30.0">
                           <children>
                              <ComboBox fx:id="cb_materiais" prefWidth="240.0">
                                 <HBox.margin>
                                    <Insets right="24.0" />
                                 </HBox.margin>
                              </ComboBox>
                              <Button mnemonicParsing="false" onAction="#onAdicionarMaterial" text="+">
                                 <HBox.margin>
                                    <Insets right="6.0" />
                                 </HBox.margin></Button>
                              <Button layoutX="160.0" layoutY="10.0" mnemonicParsing="false" onAction="#onSubtrairMaterial" text="-" />
                           </children>
                           <VBox.margin>
                              <Insets bottom="24.0" top="24.0" />
                           </VBox.margin>
                        </HBox>
                     </children>
                  </VBox>
                  <VBox layoutX="10.0" layoutY="10.0">
                     <children>
                        <TableView fx:id="tv_procedimentos">
                           <columns>
                              <TableColumn fx:id="col_procedimento" maxWidth="1.7976931348623157E308" prefWidth="404.0" text="PROCEDIMENTO" />
                              <TableColumn fx:id="col_qtde_procedimento" maxWidth="180.0" minWidth="180.0" prefWidth="180.0" text="QUANTIDADE" />
                           </columns>
                           <columnResizePolicy>
                              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                           </columnResizePolicy>
                        </TableView>
                        <HBox alignment="BOTTOM_LEFT" minHeight="30.0" prefHeight="30.0" prefWidth="570.0">
                           <children>
                              <ComboBox fx:id="cb_procedimentos" prefWidth="240.0">
                                 <HBox.margin>
                                    <Insets right="24.0" />
                                 </HBox.margin>
                              </ComboBox>
                              <Button mnemonicParsing="false" onAction="#onAdicionarProcedimento" text="+">
                                 <HBox.margin>
                                    <Insets right="6.0" />
                                 </HBox.margin></Button>
                              <Button layoutX="160.0" layoutY="10.0" mnemonicParsing="false" onAction="#onSubtrairProcedimento" text="-" />
                           </children>
                           <VBox.margin>
                              <Insets bottom="24.0" top="24.0" />
                           </VBox.margin>
                        </HBox>
                     </children>
                  </VBox>
                        <HBox alignment="CENTER" minHeight="60.0" spacing="50.0">
                            <children>
                                <Button mnemonicParsing="false" onAction="#onEfetivar" text="Efetivar Consulta">
                                    <font>
                                        <Font name="System Bold" size="14.0" />
                                    </font>
                                </Button>
                                <Button layoutX="241.0" layoutY="42.0" mnemonicParsing="false" onAction="#onCancelar" text="Cancelar Consulta">
                                    <font>
                                        <Font name="System Bold" size="14.0" />
                                    </font>
                                </Button>
                            </children>
                            <VBox.margin>
                                <Insets bottom="12.0" left="12.0" right="12.0" />
                            </VBox.margin>
                        </HBox>
                    </children>
                </VBox>
            </children>
            <VBox.margin>
                <Insets />
            </VBox.margin>
        </HBox>
    </children>
    <padding>
        <Insets bottom="60.0" left="60.0" right="60.0" top="60.0" />
    </padding>
</VBox>
