<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<VBox alignment="CENTER" prefHeight="480.0" prefWidth="480.0" spacing="24.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="matheus.bcc.dentalfx.RelatorioController">
    <children>
        <Label layoutX="183.0" layoutY="71.0" text="Historico de Consultas">
            <font>
                <Font name="System Bold" size="20.0" />
            </font>
        </Label>
        <VBox alignment="CENTER" spacing="8.0">
            <children>
                <Label text="Selecione um dentista">
                    <VBox.margin>
                        <Insets top="12.0" />
                    </VBox.margin>
                </Label>
                <ComboBox fx:id="cb_dentista" prefWidth="150.0" />
            </children>
        </VBox>
        <VBox alignment="CENTER" spacing="8.0">
            <children>
                <Label text="Data inicial" />
                <DatePicker fx:id="dp_inicial" />
            </children>
        </VBox>
        <VBox alignment="CENTER" spacing="8.0">
            <children>
                <Label text="Data final" />
                <DatePicker fx:id="dp_final" />
            </children>
        </VBox>
        <Button mnemonicParsing="false" onAction="#onGerarRelatorio" text="Gerar relatorio" />
    </children>
</VBox>
