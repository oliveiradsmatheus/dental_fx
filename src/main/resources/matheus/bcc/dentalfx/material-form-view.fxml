<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox prefHeight="390.0" prefWidth="768.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="matheus.bcc.dentalfx.MaterialFormController">
    <children>
        <VBox VBox.vgrow="ALWAYS">
            <children>
                <Label text="ID">
                    <VBox.margin>
                        <Insets top="18.0" />
                    </VBox.margin>
                </Label>
                <TextField fx:id="tf_id" editable="false" />
                <Label layoutX="10.0" layoutY="10.0" text="Descrição">
                    <VBox.margin>
                        <Insets top="18.0" />
                    </VBox.margin>
                </Label>
                <TextField fx:id="tf_descricao" layoutX="10.0" layoutY="27.0" />
                <Label layoutX="10.0" layoutY="52.0" text="Preço">
                    <VBox.margin>
                        <Insets top="18.0" />
                    </VBox.margin>
                </Label>
                <TextField fx:id="tf_preco" layoutX="10.0" layoutY="69.0" />
            </children>
            <VBox.margin>
                <Insets bottom="12.0" left="12.0" right="12.0" top="12.0" />
            </VBox.margin>
        </VBox>
        <HBox alignment="CENTER" maxHeight="96.0" prefHeight="96.0" prefWidth="768.0" spacing="48.0">
            <children>
                <Button fx:id="bt_confirmar" mnemonicParsing="false" onAction="#onConfirmar" prefHeight="33.0" prefWidth="132.0" text="Confirmar" />
                <Button fx:id="bt_cancelar" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#onCancelar" prefHeight="33.0" prefWidth="132.0" text="Cancelar" />
            </children>
        </HBox>
    </children>
    <padding>
        <Insets left="12.0" right="12.0" top="12.0" />
    </padding>
</VBox>
